import pandas as pd

def count_salary_categories(accounts: pd.DataFrame) -> pd.DataFrame:
    a, b = (accounts.income<20000).sum(),  (accounts.income>50000).sum()
    return pd.DataFrame({
        "category": ["Low Salary", "Average Salary", "High Salary"],
        "accounts_count": [int(a), len(accounts)-int(a)-int(b), int(b)]
    })

    